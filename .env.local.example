# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Google Maps
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# API Security
INTERNAL_API_KEY=generate-with-openssl-rand-hex-32
PARTNER_API_KEY=generate-with-openssl-rand-hex-32  
MOBILE_APP_KEY=generate-with-openssl-rand-hex-32
PERSONAL_NUMBER_SALT=generate-secure-salt-here

# SendGrid (for notifications)
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@nordflytt.se

# Twilio (for SMS)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+46xxxxxxxxx
TWILIO_ALPHA_SENDER=Nordflytt

# App URL (for links in emails)
NEXT_PUBLIC_APP_URL=https://your-domain.com

# Creditsafe (for credit checks)
CREDITSAFE_USERNAME=your-username
CREDITSAFE_PASSWORD=your-password

# Monitoring & Analytics
# Sentry
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn
SENTRY_ORG=your-sentry-org
SENTRY_PROJECT=your-sentry-project
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# Google Analytics 4
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Custom Monitoring Endpoint (optional)
NEXT_PUBLIC_MONITORING_ENDPOINT=https://your-monitoring-endpoint.com/api/metrics

# Encryption Key (for sensitive data)
ENCRYPTION_KEY=generate-32-byte-key-with-openssl-rand-hex-32