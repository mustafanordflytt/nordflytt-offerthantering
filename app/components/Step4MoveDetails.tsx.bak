// üîß F√ñRB√ÑTTRAD DEBOUNCE - Mer aggressiv f√∂r att stoppa API-spam
function debounce<F extends (...args: any[]) => any>(func: F, wait: number = 800) {
  let timeout: NodeJS.Timeout | null = null
  let lastExecuted = 0

  return ((...args: Parameters<F>) => {
    const now = Date.now()
    
    if (timeout) {
      clearTimeout(timeout)
    }

    // üîß L√ÑNGRE V√ÑNTETID - F√∂rhindra f√∂r m√•nga anrop
    if (now - lastExecuted > wait * 3) { // √Ñndrat fr√•n *2 till *3
      lastExecuted = now
      func(...args)
      return
    }

    timeout = setTimeout(() => {
      lastExecuted = Date.now()
      func(...args)
    }, wait)
  }) as F
}

