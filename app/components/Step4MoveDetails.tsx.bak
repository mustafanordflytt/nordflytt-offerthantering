                <p>Distance: {formData.calculatedDistance || 'Ej ber√§knad'}</p>
                <p>API Spam Protection: {isFormValidating ? 'üîí BLOCKED' : 'üü¢ ACTIVE'}</p>
              </div>
            )}

            <div className="flex justify-between">
              <button
                type="button"
                onClick={prevStep}
                className="inline-flex items-center px-4 py-2 border border-gray-300 text-sm font-medium rounded-md text-gray-700 bg-white hover:bg-gray-50"
              >
                Tillbaka
              </button>
              <button
                type="submit"
                disabled={!isFormValid || isFormValidating}
                className={`inline-flex items-center px-4 py-2 border border-transparent text-sm font-medium rounded-md shadow-sm text-white ${
                  isFormValid && !isFormValidating
                    ? 'bg-blue-600 hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-blue-500' 
                    : 'bg-gray-400 cursor-not-allowed'
                }`}
              >
                {isFormValidating ? 'Validerar...' : 'N√§sta'}
                {(!isFormValid || isFormValidating) && (
                  <AlertTriangle className="ml-2 w-4 h-4" />
                )}
              </button>
            </div>
          </div>
        </form>
      </Suspense>
    </ErrorBoundary>
  )
}