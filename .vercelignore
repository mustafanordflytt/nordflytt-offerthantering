*.test.js
*.test.ts
test-*
*-test.*
__tests__
e2e

# Development files
*.log
*.md
!README.md
.env.local
.env.development*
.env.test*

# Scripts and configs
scripts/
*.sh
jest.config.*
playwright.config.*
cypress.config.*

# Documentation
docs/
*.png
*.jpg
*.jpeg
*.gif
*.svg
!public/**/*.svg
!public/**/*.png
!public/**/*.jpg

# Temporary files
tmp/
temp/
*.tmp
*.temp

# CI/CD
.github/
.circleci/
.travis.yml

# IDE
.vscode/
.idea/
*.sublime-*

# Mac
.DS_Store

# Debug files
debug-*
*-debug.*

# Test data
test-data/
mock-data/
fixtures/

# Python files (not needed for Next.js)
*.py
__pycache__/
*.pyc
requirements.txt

# Unnecessary config files
babel.config.js.backup
tsconfig.tsbuildinfo

# Large unnecessary files
*.tar.gz
*.zip
contracts/
generated-documents/
public/contracts/
public/generated-documents/