# AWS SageMaker Configuration for Nordflytt ML Model
# Copy this to .env.local and fill in your actual values

# AWS Credentials (use IAM role in production)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=eu-north-1

# SageMaker Endpoint Configuration
SAGEMAKER_ENDPOINT_NAME=nordflytt-time-estimation-prod
SAGEMAKER_ROLE_ARN=arn:aws:iam::YOUR_ACCOUNT_ID:role/SageMakerExecutionRole

# Model Configuration
ML_MODEL_VERSION=v1.0-randomforest
ML_CONFIDENCE_THRESHOLD=0.85
ML_ENABLE_FALLBACK=true

# Monitoring Configuration
SAGEMAKER_CAPTURE_DATA=true
SAGEMAKER_CAPTURE_PERCENTAGE=10

# Feature Flags
ENABLE_ML_PREDICTIONS=true
ENABLE_A_B_TESTING=true
ML_ROLLOUT_PERCENTAGE=100